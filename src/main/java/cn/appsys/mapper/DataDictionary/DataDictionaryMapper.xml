<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.appsys.mapper.DataDictionary.DataDictionaryMapper">
	<select id="getDataDictionary" resultType="Data_dictionary">
		SELECT * FROM data_dictionary WHERE typeCode='APP_FLATFORM'
	</select>
	<select id="getDataDictionaryStatus" resultType="Data_dictionary">
		SELECT * FROM data_dictionary WHERE typeCode='APP_STATUS'
	</select>
</mapper>